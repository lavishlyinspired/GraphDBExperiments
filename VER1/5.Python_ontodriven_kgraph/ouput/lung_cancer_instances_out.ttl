@prefix ont: <http://lungkg.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix res: <http://lungkg.org/resource/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ont:Chemotherapy a owl:Class ;
    rdfs:subClassOf ont:Therapy .

ont:EGFR_Guideline a ont:Guideline ;
    rdfs:label "EGFR positive â†’ Osimertinib" ;
    ont:recommends ont:EGFR_Targeted_Therapy .

ont:GenomicTest a owl:Class ;
    rdfs:subClassOf ont:Test .

ont:Imaging a owl:Class ;
    rdfs:subClassOf ont:Test .

ont:Immunotherapy a owl:Class ;
    rdfs:subClassOf ont:Therapy .

ont:LungCancer a owl:Class ;
    rdfs:subClassOf ont:Disease .

ont:Ontology a owl:Ontology ;
    rdfs:label "Lung Cancer Decision Support Knowledge Graph Ontology" ;
    rdfs:comment "Ontology for lung cancer diagnosis, genomics, treatment recommendation and outcomes reasoning" .

ont:Pathology a owl:Class ;
    rdfs:subClassOf ont:Test .

ont:Stage_I a ont:Stage ;
    ont:stageName "I" .

ont:Stage_II a ont:Stage ;
    ont:stageName "II" .

ont:Stage_III a ont:Stage ;
    ont:stageName "III" .

ont:Stage_IV a ont:Stage ;
    ont:stageName "IV" .

ont:Surgery a owl:Class ;
    rdfs:subClassOf ont:Therapy .

ont:age a owl:DatatypeProperty ;
    rdfs:domain ont:Patient ;
    rdfs:range xsd:integer .

ont:diagnosedWith a owl:ObjectProperty ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Disease .

ont:drugName a owl:DatatypeProperty ;
    rdfs:domain ont:Drug ;
    rdfs:range xsd:string .

ont:geneName a owl:DatatypeProperty ;
    rdfs:domain ont:Gene ;
    rdfs:range xsd:string .

ont:hasGene a owl:ObjectProperty ;
    rdfs:domain ont:Biomarker ;
    rdfs:range ont:Gene .

ont:hasHistology a owl:ObjectProperty ;
    rdfs:domain ont:Tumor ;
    rdfs:range ont:Histology .

ont:hasMutation a owl:ObjectProperty ;
    rdfs:domain ont:Biomarker ;
    rdfs:range ont:Mutation .

ont:hasOutcome a owl:ObjectProperty ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Outcome .

ont:hasStage a owl:ObjectProperty ;
    rdfs:domain ont:Tumor ;
    rdfs:range ont:Stage .

ont:hasTumor a owl:ObjectProperty ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Tumor .

ont:histologyName a owl:DatatypeProperty ;
    rdfs:domain ont:Histology ;
    rdfs:range xsd:string .

ont:patientId a owl:DatatypeProperty ;
    rdfs:domain ont:Patient ;
    rdfs:range xsd:string .

ont:receivedTherapy a owl:ObjectProperty ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Therapy .

ont:recommends a owl:ObjectProperty ;
    rdfs:domain ont:Guideline ;
    rdfs:range ont:Therapy .

ont:responseType a owl:DatatypeProperty ;
    rdfs:domain ont:Outcome ;
    rdfs:range xsd:string .

ont:sex a owl:DatatypeProperty ;
    rdfs:domain ont:Patient ;
    rdfs:range xsd:string .

ont:smokingPackYears a owl:DatatypeProperty ;
    rdfs:domain ont:Patient ;
    rdfs:range xsd:float .

ont:stageName a owl:DatatypeProperty ;
    rdfs:domain ont:Stage ;
    rdfs:range xsd:string .

ont:survivalMonths a owl:DatatypeProperty ;
    rdfs:domain ont:Outcome ;
    rdfs:range xsd:float .

ont:testedForBiomarker a owl:ObjectProperty ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Biomarker .

ont:triggers a owl:ObjectProperty ;
    rdfs:domain ont:ClinicalRule ;
    rdfs:range ont:Therapy .

ont:underwentTest a owl:ObjectProperty ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Test .

ont:usesDrug a owl:ObjectProperty ;
    rdfs:domain ont:Therapy ;
    rdfs:range ont:Drug .

