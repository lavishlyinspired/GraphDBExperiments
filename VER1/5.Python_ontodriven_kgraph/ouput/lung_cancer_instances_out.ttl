@prefix ont: <http://lungkg.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix res: <http://lungkg.org/resource/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ont:Chemotherapy a owl:Class ;
    rdfs:label "Chemotherapy" ;
    rdfs:comment "Chemotherapy treatment" ;
    rdfs:subClassOf ont:Therapy .

ont:EGFR_Guideline a ont:Guideline ;
    rdfs:label "EGFR positive to Osimertinib" ;
    ont:recommends ont:EGFR_Targeted_Therapy ;
    rdfs:comment "Guideline for EGFR positive patients" .

ont:GenomicTest a owl:Class ;
    rdfs:label "Genomic Test" ;
    rdfs:comment "Genomic or molecular testing" ;
    rdfs:subClassOf ont:Test .

ont:Imaging a owl:Class ;
    rdfs:label "Imaging" ;
    rdfs:comment "Imaging test" ;
    rdfs:subClassOf ont:Test .

ont:Immunotherapy a owl:Class ;
    rdfs:label "Immunotherapy" ;
    rdfs:comment "Immunotherapy treatment" ;
    rdfs:subClassOf ont:Therapy .

ont:LungCancer a owl:Class ;
    rdfs:label "Lung Cancer" ;
    rdfs:comment "Lung cancer disease" ;
    rdfs:subClassOf ont:Disease .

ont:Ontology a owl:Ontology ;
    rdfs:label "Lung Cancer Decision Support Knowledge Graph Ontology" ;
    rdfs:comment "Ontology for lung cancer diagnosis, genomics, treatment recommendation and outcomes reasoning" .

ont:Pathology a owl:Class ;
    rdfs:label "Pathology" ;
    rdfs:comment "Pathology test" ;
    rdfs:subClassOf ont:Test .

ont:Stage_I a ont:Stage ;
    rdfs:label "Stage I" ;
    ont:name "I" .

ont:Stage_II a ont:Stage ;
    rdfs:label "Stage II" ;
    ont:name "II" .

ont:Stage_III a ont:Stage ;
    rdfs:label "Stage III" ;
    ont:name "III" .

ont:Stage_IV a ont:Stage ;
    rdfs:label "Stage IV" ;
    ont:name "IV" .

ont:Surgery a owl:Class ;
    rdfs:label "Surgery" ;
    rdfs:comment "Surgical treatment" ;
    rdfs:subClassOf ont:Therapy .

ont:age a owl:DatatypeProperty ;
    rdfs:label "age" ;
    rdfs:comment "Patient age in years" ;
    rdfs:domain ont:Patient ;
    rdfs:range xsd:integer .

ont:description a owl:DatatypeProperty ;
    rdfs:label "description" ;
    rdfs:comment "Description of an entity" ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

ont:diagnosedWith a owl:ObjectProperty ;
    rdfs:label "diagnosed with" ;
    rdfs:comment "Patient diagnosed with a disease" ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Disease .

ont:hasGene a owl:ObjectProperty ;
    rdfs:label "has gene" ;
    rdfs:comment "Biomarker associated with a gene" ;
    rdfs:domain ont:Biomarker ;
    rdfs:range ont:Gene .

ont:hasHistology a owl:ObjectProperty ;
    rdfs:label "has histology" ;
    rdfs:comment "Tumor has a histological type" ;
    rdfs:domain ont:Tumor ;
    rdfs:range ont:Histology .

ont:hasMutation a owl:ObjectProperty ;
    rdfs:label "has mutation" ;
    rdfs:comment "Biomarker has a mutation" ;
    rdfs:domain ont:Biomarker ;
    rdfs:range ont:Mutation .

ont:hasOutcome a owl:ObjectProperty ;
    rdfs:label "has outcome" ;
    rdfs:comment "Patient has a treatment outcome" ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Outcome .

ont:hasStage a owl:ObjectProperty ;
    rdfs:label "has stage" ;
    rdfs:comment "Tumor has a cancer stage" ;
    rdfs:domain ont:Tumor ;
    rdfs:range ont:Stage .

ont:hasTumor a owl:ObjectProperty ;
    rdfs:label "has tumor" ;
    rdfs:comment "Patient has a tumor" ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Tumor .

ont:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:comment "Common name property for entities" ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string .

ont:patientId a owl:DatatypeProperty ;
    rdfs:label "patient ID" ;
    rdfs:comment "Unique patient identifier" ;
    rdfs:domain ont:Patient ;
    rdfs:range xsd:string .

ont:receivedTherapy a owl:ObjectProperty ;
    rdfs:label "received therapy" ;
    rdfs:comment "Patient received a therapy" ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Therapy .

ont:recommends a owl:ObjectProperty ;
    rdfs:label "recommends" ;
    rdfs:comment "Guideline recommends a therapy" ;
    rdfs:domain ont:Guideline ;
    rdfs:range ont:Therapy .

ont:responseType a owl:DatatypeProperty ;
    rdfs:label "response type" ;
    rdfs:comment "Treatment response classification" ;
    rdfs:domain ont:Outcome ;
    rdfs:range xsd:string .

ont:sex a owl:DatatypeProperty ;
    rdfs:label "sex" ;
    rdfs:comment "Patient biological sex" ;
    rdfs:domain ont:Patient ;
    rdfs:range xsd:string .

ont:smokingPackYears a owl:DatatypeProperty ;
    rdfs:label "smoking pack years" ;
    rdfs:comment "Cumulative smoking exposure in pack-years" ;
    rdfs:domain ont:Patient ;
    rdfs:range xsd:float .

ont:survivalMonths a owl:DatatypeProperty ;
    rdfs:label "survival months" ;
    rdfs:comment "Patient survival duration in months" ;
    rdfs:domain ont:Outcome ;
    rdfs:range xsd:float .

ont:testedForBiomarker a owl:ObjectProperty ;
    rdfs:label "tested for biomarker" ;
    rdfs:comment "Patient tested for a biomarker" ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Biomarker .

ont:triggers a owl:ObjectProperty ;
    rdfs:label "triggers" ;
    rdfs:comment "Clinical rule triggers a therapy recommendation" ;
    rdfs:domain ont:ClinicalRule ;
    rdfs:range ont:Therapy .

ont:underwentTest a owl:ObjectProperty ;
    rdfs:label "underwent test" ;
    rdfs:comment "Patient underwent a diagnostic test" ;
    rdfs:domain ont:Patient ;
    rdfs:range ont:Test .

ont:usesDrug a owl:ObjectProperty ;
    rdfs:label "uses drug" ;
    rdfs:comment "Therapy uses a drug" ;
    rdfs:domain ont:Therapy ;
    rdfs:range ont:Drug .

