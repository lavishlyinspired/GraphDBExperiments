ont:ClinicalRule a owl:Class .

ont:EGFR_Targeted_Therapy a ont:TargetedTherapy ;
    ont:usesDrug ont:Osimertinib .

ont:Mutation a owl:Class .

ont:Osimertinib a ont:Drug ;
    ont:drugName "Osimertinib" .

ont:TargetedTherapy a owl:Class ;
    rdfs:subClassOf ont:Therapy .

ont:Disease a owl:Class .

ont:Gene a owl:Class .

ont:Guideline a owl:Class .

ont:Histology a owl:Class .

ont:Biomarker a owl:Class .

ont:Drug a owl:Class .

ont:Outcome a owl:Class .

ont:Tumor a owl:Class .

ont:Test a owl:Class .

ont:Stage a owl:Class .

ont:Therapy a owl:Class .

ont:Patient a owl:Class .

