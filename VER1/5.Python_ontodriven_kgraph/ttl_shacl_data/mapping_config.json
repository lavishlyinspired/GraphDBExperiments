{
  "patients": {
    "file": "ttl_shacl_data/lung_patients.csv",
    "subject": "Patient_{patient_id}",
    "type": "Patient",
    "datatype_props": {
      "patientId": "patient_id",
      "age": "age",
      "sex": "sex",
      "smokingPackYears": "smoking_pack_years"
    },
    "object_links": {
      "diagnosedWith": "LungCancer",
      "hasTumor": "Tumor_{patient_id}",
      "hasStage": "Stage_{stage}",
      "hasHistology": "Histology_{histology}"
    }
  },

  "tumors": {
    "file": "ttl_shacl_data/lung_patients.csv",
    "subject": "Tumor_{patient_id}",
    "type": "Tumor",
    "object_links": {
      "hasStage": "Stage_{stage}",
      "hasHistology": "Histology_{histology}"
    }
  },

  "mutations": {
    "file": "ttl_shacl_data/lung_mutations.csv",
    "subject": "Patient_{patient_id}",
    "object_links": {
      "testedForBiomarker": "Biomarker_{gene}",
      "underwentTest": "GenomicTest_{patient_id}"
    }
  },

  "genomic_tests": {
    "file": "ttl_shacl_data/lung_mutations.csv",
    "subject": "GenomicTest_{patient_id}",
    "type": "GenomicTest",
    "object_links": {}
  },

  "biomarkers": {
    "file": "ttl_shacl_data/lung_mutations.csv",
    "subject": "Biomarker_{gene}",
    "type": "Biomarker",
    "object_links": {
      "hasGene": "Gene_{gene}",
      "hasMutation": "Mutation_{mutation}"
    }
  },

  "treatments": {
    "file": "ttl_shacl_data/lung_treatments.csv",
    "subject": "Patient_{patient_id}",
    "object_links": {
      "receivedTherapy": "Therapy_{patient_id}_{drug}",
      "hasOutcome": "Outcome_{patient_id}_{response}"
    }
  },

  "therapies": {
    "file": "ttl_shacl_data/lung_treatments.csv",
    "subject": "Therapy_{patient_id}_{drug}",
    "type": "Therapy",
    "object_links": {
      "usesDrug": "Drug_{drug}"
    }
  }
}
