@prefix : <http://lungkg.org/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################
# Ontology
#################################################

:Ontology rdf:type owl:Ontology ;
    rdfs:label "Lung Cancer Decision Support Knowledge Graph Ontology" ;
    rdfs:comment "Ontology for lung cancer diagnosis, genomics, treatment recommendation and outcomes reasoning" .

#################################################
# Core Classes
#################################################

:Patient rdf:type owl:Class ;
    rdfs:label "Patient" ;
    rdfs:comment "A patient in the lung cancer knowledge graph" .

:Disease rdf:type owl:Class ;
    rdfs:label "Disease" ;
    rdfs:comment "A disease or medical condition" .

:LungCancer rdf:type owl:Class ;
    rdfs:subClassOf :Disease ;
    rdfs:label "Lung Cancer" ;
    rdfs:comment "Lung cancer disease" .

:Tumor rdf:type owl:Class ;
    rdfs:label "Tumor" ;
    rdfs:comment "A tumor or malignant growth" .

:Stage rdf:type owl:Class ;
    rdfs:label "Stage" ;
    rdfs:comment "Cancer staging classification" .

:Histology rdf:type owl:Class ;
    rdfs:label "Histology" ;
    rdfs:comment "Histological type of cancer" .

:Biomarker rdf:type owl:Class ;
    rdfs:label "Biomarker" ;
    rdfs:comment "Biological marker for genomic analysis" .

:Mutation rdf:type owl:Class ;
    rdfs:label "Mutation" ;
    rdfs:comment "Genetic mutation" .

:Gene rdf:type owl:Class ;
    rdfs:label "Gene" ;
    rdfs:comment "A gene" .

:Test rdf:type owl:Class ;
    rdfs:label "Test" ;
    rdfs:comment "A clinical or diagnostic test" .

:Imaging rdf:type owl:Class ;
    rdfs:subClassOf :Test ;
    rdfs:label "Imaging" ;
    rdfs:comment "Imaging test" .

:Pathology rdf:type owl:Class ;
    rdfs:subClassOf :Test ;
    rdfs:label "Pathology" ;
    rdfs:comment "Pathology test" .

:GenomicTest rdf:type owl:Class ;
    rdfs:subClassOf :Test ;
    rdfs:label "Genomic Test" ;
    rdfs:comment "Genomic or molecular testing" .

:Therapy rdf:type owl:Class ;
    rdfs:label "Therapy" ;
    rdfs:comment "A medical therapy or treatment" .

:Drug rdf:type owl:Class ;
    rdfs:label "Drug" ;
    rdfs:comment "A pharmaceutical drug" .

:Chemotherapy rdf:type owl:Class ;
    rdfs:subClassOf :Therapy ;
    rdfs:label "Chemotherapy" ;
    rdfs:comment "Chemotherapy treatment" .

:TargetedTherapy rdf:type owl:Class ;
    rdfs:subClassOf :Therapy ;
    rdfs:label "Targeted Therapy" ;
    rdfs:comment "Targeted therapy treatment" .

:Immunotherapy rdf:type owl:Class ;
    rdfs:subClassOf :Therapy ;
    rdfs:label "Immunotherapy" ;
    rdfs:comment "Immunotherapy treatment" .

:Surgery rdf:type owl:Class ;
    rdfs:subClassOf :Therapy ;
    rdfs:label "Surgery" ;
    rdfs:comment "Surgical treatment" .

:Outcome rdf:type owl:Class ;
    rdfs:label "Outcome" ;
    rdfs:comment "Treatment outcome" .

:Guideline rdf:type owl:Class ;
    rdfs:label "Guideline" ;
    rdfs:comment "Clinical guideline" .

:ClinicalRule rdf:type owl:Class ;
    rdfs:label "Clinical Rule" ;
    rdfs:comment "Clinical decision rule" .

:Article rdf:type owl:Class ;
    rdfs:label "Article" ;
    rdfs:comment "Biomedical literature or case report" .

:Entity rdf:type owl:Class ;
    rdfs:label "Entity" ;
    rdfs:comment "Named entity extracted from text" .

#################################################
# Datatype Properties
#################################################

:sourceUrl rdf:type owl:DatatypeProperty ;
    rdfs:domain :Article ;
    rdfs:range xsd:string ;
    rdfs:label "source URL" ;
    rdfs:comment "Article source URL or URI" .

:title rdf:type owl:DatatypeProperty ;
    rdfs:domain :Article ;
    rdfs:range xsd:string ;
    rdfs:label "title" ;
    rdfs:comment "Article title" .

:body rdf:type owl:DatatypeProperty ;
    rdfs:domain :Article ;
    rdfs:range xsd:string ;
    rdfs:label "body" ;
    rdfs:comment "Article body text" .

:publicationDate rdf:type owl:DatatypeProperty ;
    rdfs:domain :Article ;
    rdfs:range xsd:date ;
    rdfs:label "publication date" ;
    rdfs:comment "Article publication date" .

:entityType rdf:type owl:DatatypeProperty ;
    rdfs:domain :Entity ;
    rdfs:range xsd:string ;
    rdfs:label "entity type" ;
    rdfs:comment "Type of named entity (PERSON, LOCATION, etc.)" .

:salience rdf:type owl:DatatypeProperty ;
    rdfs:domain :Entity ;
    rdfs:range xsd:float ;
    rdfs:label "salience" ;
    rdfs:comment "Entity importance score from NLP" .

#################################################
# Datatype Properties (continued)
#################################################

:patientId rdf:type owl:DatatypeProperty ;
    rdfs:domain :Patient ;
    rdfs:range xsd:string ;
    rdfs:label "patient ID" ;
    rdfs:comment "Unique patient identifier" .

:age rdf:type owl:DatatypeProperty ;
    rdfs:domain :Patient ;
    rdfs:range xsd:integer ;
    rdfs:label "age" ;
    rdfs:comment "Patient age in years" .

:sex rdf:type owl:DatatypeProperty ;
    rdfs:domain :Patient ;
    rdfs:range xsd:string ;
    rdfs:label "sex" ;
    rdfs:comment "Patient biological sex" .

:smokingPackYears rdf:type owl:DatatypeProperty ;
    rdfs:domain :Patient ;
    rdfs:range xsd:float ;
    rdfs:label "smoking pack years" ;
    rdfs:comment "Cumulative smoking exposure in pack-years" .

:name rdf:type owl:DatatypeProperty ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string ;
    rdfs:label "name" ;
    rdfs:comment "Common name property for entities" .

:description rdf:type owl:DatatypeProperty ;
    rdfs:domain owl:Thing ;
    rdfs:range xsd:string ;
    rdfs:label "description" ;
    rdfs:comment "Description of an entity" .

:responseType rdf:type owl:DatatypeProperty ;
    rdfs:domain :Outcome ;
    rdfs:range xsd:string ;
    rdfs:label "response type" ;
    rdfs:comment "Treatment response classification" .

:survivalMonths rdf:type owl:DatatypeProperty ;
    rdfs:domain :Outcome ;
    rdfs:range xsd:float ;
    rdfs:label "survival months" ;
    rdfs:comment "Patient survival duration in months" .

#################################################
# Object Properties
#################################################

:diagnosedWith rdf:type owl:ObjectProperty ;
    rdfs:domain :Patient ;
    rdfs:range :Disease ;
    rdfs:label "diagnosed with" ;
    rdfs:comment "Patient diagnosed with a disease" .

:hasTumor rdf:type owl:ObjectProperty ;
    rdfs:domain :Patient ;
    rdfs:range :Tumor ;
    rdfs:label "has tumor" ;
    rdfs:comment "Patient has a tumor" .

:hasStage rdf:type owl:ObjectProperty ;
    rdfs:domain :Tumor ;
    rdfs:range :Stage ;
    rdfs:label "has stage" ;
    rdfs:comment "Tumor has a cancer stage" .

:hasHistology rdf:type owl:ObjectProperty ;
    rdfs:domain :Tumor ;
    rdfs:range :Histology ;
    rdfs:label "has histology" ;
    rdfs:comment "Tumor has a histological type" .

:underwentTest rdf:type owl:ObjectProperty ;
    rdfs:domain :Patient ;
    rdfs:range :Test ;
    rdfs:label "underwent test" ;
    rdfs:comment "Patient underwent a diagnostic test" .

:testedForBiomarker rdf:type owl:ObjectProperty ;
    rdfs:domain :Patient ;
    rdfs:range :Biomarker ;
    rdfs:label "tested for biomarker" ;
    rdfs:comment "Patient tested for a biomarker" .

:hasGene rdf:type owl:ObjectProperty ;
    rdfs:domain :Biomarker ;
    rdfs:range :Gene ;
    rdfs:label "has gene" ;
    rdfs:comment "Biomarker associated with a gene" .

:hasMutation rdf:type owl:ObjectProperty ;
    rdfs:domain :Biomarker ;
    rdfs:range :Mutation ;
    rdfs:label "has mutation" ;
    rdfs:comment "Biomarker has a mutation" .

:receivedTherapy rdf:type owl:ObjectProperty ;
    rdfs:domain :Patient ;
    rdfs:range :Therapy ;
    rdfs:label "received therapy" ;
    rdfs:comment "Patient received a therapy" .

:usesDrug rdf:type owl:ObjectProperty ;
    rdfs:domain :Therapy ;
    rdfs:range :Drug ;
    rdfs:label "uses drug" ;
    rdfs:comment "Therapy uses a drug" .

:hasOutcome rdf:type owl:ObjectProperty ;
    rdfs:domain :Patient ;
    rdfs:range :Outcome ;
    rdfs:label "has outcome" ;
    rdfs:comment "Patient has a treatment outcome" .

:recommends rdf:type owl:ObjectProperty ;
    rdfs:domain :Guideline ;
    rdfs:range :Therapy ;
    rdfs:label "recommends" ;
    rdfs:comment "Guideline recommends a therapy" .

:triggers rdf:type owl:ObjectProperty ;
    rdfs:domain :ClinicalRule ;
    rdfs:range :Therapy ;
    rdfs:label "triggers" ;
    rdfs:comment "Clinical rule triggers a therapy recommendation" .

:refersTo rdf:type owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:range owl:Thing ;
    rdfs:label "refers to" ;
    rdfs:comment "Article refers to an ontology concept" .

:mentions rdf:type owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:range :Entity ;
    rdfs:label "mentions" ;
    rdfs:comment "Article mentions a named entity" .

:about rdf:type owl:ObjectProperty ;
    rdfs:domain :Article ;
    rdfs:range owl:Thing ;
    rdfs:label "about" ;
    rdfs:comment "Article is about a topic or concept" .

#################################################
# DSS Rule Examples (optional individuals)
#################################################

:EGFR_Guideline rdf:type :Guideline ;
    rdfs:label "EGFR positive to Osimertinib" ;
    rdfs:comment "Guideline for EGFR positive patients" .

:Osimertinib rdf:type :Drug ;
    :name "Osimertinib" ;
    rdfs:label "Osimertinib" .

:EGFR_Targeted_Therapy rdf:type :TargetedTherapy ;
    :usesDrug :Osimertinib ;
    rdfs:label "EGFR Targeted Therapy" .

:EGFR_Guideline :recommends :EGFR_Targeted_Therapy .

#################################################
# Stage Instances (optional controlled vocabulary)
#################################################

:Stage_I rdf:type :Stage ;
    :name "I" ;
    rdfs:label "Stage I" .

:Stage_II rdf:type :Stage ;
    :name "II" ;
    rdfs:label "Stage II" .

:Stage_III rdf:type :Stage ;
    :name "III" ;
    rdfs:label "Stage III" .

:Stage_IV rdf:type :Stage ;
    :name "IV" ;
    rdfs:label "Stage IV" .
